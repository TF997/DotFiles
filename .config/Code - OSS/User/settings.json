{
    "workbench.colorTheme": "Wal",
    "window.menuBarVisibility": "toggle",
    "editor.fontFamily": "Operator Mono Lig,'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 18,
    "editor.fontLigatures": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    "comment",
                    "entity.name.type.class",
                    "keyword",
                    "constant",
                    "storage.modifier",
                    "storage.type.class.js"
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    "invalid",
                    "keyword.operator",
                    "constant.numeric.css",
                    "keyword.other.unit.px.css",
                    "constant.numeric.decimal.js",
                    "constant.numeric.json"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ],
        "[Wal]": {
            "textMateRules": [
                {
                    "settings": {
                        "background": "#162030",
                        "foreground": "#EAE5D4"
                    }
                },
                {
                    "name": "Comment",
                    "scope": [
                        "comment"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#A3A094b0"
                    }
                },
                {
                    "name": "String",
                    "scope": [
                        "string",
                        "constant.other.symbol"
                    ],
                    "settings": {
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "name": "Regular Expressions and Escape Characters",
                    "scope": [
                        "string.regexp",
                        "constant.character",
                        "constant.other"
                    ],
                    "settings": {
                        "foreground": "#E6DCB9"
                    }
                },
                {
                    "name": "Number",
                    "scope": [
                        "constant.numeric"
                    ],
                    "settings": {
                        "foreground": "#66AFA0"
                    }
                },
                {
                    "name": "Built-in constants",
                    "scope": [
                        "constant.language"
                    ],
                    "settings": {
                        "foreground": "#66AFA0"
                    }
                },
                {
                    "name": "Variable",
                    "scope": [
                        "variable"
                    ],
                    "settings": {
                        "foreground": "#EAE5D4"
                    }
                },
                {
                    "name": "Member Variable",
                    "scope": [
                        "variable.member"
                    ],
                    "settings": {
                        "foreground": "#66AFA0"
                    }
                },
                {
                    "name": "Language variable",
                    "scope": [
                        "variable.language"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "name": "Storage",
                    "scope": [
                        "storage"
                    ],
                    "settings": {
                        "foreground": "#E6DCB9"
                    }
                },
                {
                    "name": "Keyword",
                    "scope": [
                        "keyword"
                    ],
                    "settings": {
                        "foreground": "#E6DCB9"
                    }
                },
                {
                    "name": "Operators",
                    "scope": [
                        "keyword.operator"
                    ],
                    "settings": {
                        "foreground": "#CAB2A1"
                    }
                },
                {
                    "name": "Separators like ; or ,",
                    "scope": [
                        "punctuation.separator",
                        "punctuation.terminator"
                    ],
                    "settings": {
                        "foreground": "#EAE5D4b0"
                    }
                },
                {
                    "name": "Punctuation",
                    "scope": [
                        "punctuation.section"
                    ],
                    "settings": {
                        "foreground": "#EAE5D4"
                    }
                },
                {
                    "name": "Accessor",
                    "scope": [
                        "punctuation.accessor"
                    ],
                    "settings": {
                        "foreground": "#CAB2A1"
                    }
                },
                {
                    "name": "Types fixes",
                    "scope": [
                        "source.java storage.type",
                        "source.haskell storage.type",
                        "source.c storage.type"
                    ],
                    "settings": {
                        "foreground": "#A4C6A9"
                    }
                },
                {
                    "name": "Inherited class type",
                    "scope": [
                        "entity.other.inherited-class"
                    ],
                    "settings": {
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "name": "Lambda arrow",
                    "scope": [
                        "storage.type.function"
                    ],
                    "settings": {
                        "foreground": "#E6DCB9"
                    }
                },
                {
                    "name": "Java primitive variable types",
                    "scope": [
                        "source.java storage.type.primitive"
                    ],
                    "settings": {
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "name": "Function name",
                    "scope": [
                        "entity.name.function"
                    ],
                    "settings": {
                        "foreground": "#CAB2A1"
                    }
                },
                {
                    "name": "Function arguments",
                    "scope": [
                        "variable.parameter",
                        "meta.parameter"
                    ],
                    "settings": {
                        "foreground": "#D5D2B1"
                    }
                },
                {
                    "name": "Function call",
                    "scope": [
                        "variable.function",
                        "variable.annotation",
                        "meta.function-call.generic",
                        "support.function.go"
                    ],
                    "settings": {
                        "foreground": "#CAB2A1"
                    }
                },
                {
                    "name": "Library function",
                    "scope": [
                        "support.function",
                        "support.macro"
                    ],
                    "settings": {
                        "foreground": "#66AFA0"
                    }
                },
                {
                    "name": "Imports and packages",
                    "scope": [
                        "entity.name.import",
                        "entity.name.package"
                    ],
                    "settings": {
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "name": "Entity name",
                    "scope": [
                        "entity.name"
                    ],
                    "settings": {
                        "foreground": "#A4C6A9"
                    }
                },
                {
                    "name": "Tag",
                    "scope": [
                        "entity.name.tag",
                        "meta.tag.sgml"
                    ],
                    "settings": {
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "name": "Tag start/end",
                    "scope": [
                        "punctuation.definition.tag.end",
                        "punctuation.definition.tag.begin",
                        "punctuation.definition.tag"
                    ],
                    "settings": {
                        "foreground": "#9FAB9977"
                    }
                },
                {
                    "name": "Tag attribute",
                    "scope": [
                        "entity.other.attribute-name"
                    ],
                    "settings": {
                        "foreground": "#CAB2A1"
                    }
                },
                {
                    "name": "Library constant",
                    "scope": [
                        "support.constant"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#CAB2A1"
                    }
                },
                {
                    "name": "Library class/type",
                    "scope": [
                        "support.type",
                        "support.class",
                        "source.go storage.type"
                    ],
                    "settings": {
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "name": "Decorators/annotation",
                    "scope": [
                        "meta.decorator variable.other",
                        "meta.decorator punctuation.decorator",
                        "storage.type.annotation"
                    ],
                    "settings": {
                        "foreground": "#D5D2B1"
                    }
                },
                {
                    "name": "Invalid",
                    "scope": [
                        "invalid"
                    ],
                    "settings": {
                        "foreground": "#A4C6A9"
                    }
                },
                {
                    "name": "diff.header",
                    "scope": [
                        "meta.diff",
                        "meta.diff.header"
                    ],
                    "settings": {
                        "foreground": "#D5D2B1"
                    }
                },
                {
                    "name": "Ruby class methods",
                    "scope": [
                        "source.ruby variable.other.readwrite"
                    ],
                    "settings": {
                        "foreground": "#CAB2A1"
                    }
                },
                {
                    "name": "CSS tag names",
                    "scope": [
                        "source.css entity.name.tag",
                        "source.sass entity.name.tag",
                        "source.scss entity.name.tag",
                        "source.less entity.name.tag",
                        "source.stylus entity.name.tag"
                    ],
                    "settings": {
                        "foreground": "#A4C6A9"
                    }
                },
                {
                    "name": "CSS browser prefix",
                    "scope": [
                        "source.css support.type",
                        "source.sass support.type",
                        "source.scss support.type",
                        "source.less support.type",
                        "source.stylus support.type"
                    ],
                    "settings": {
                        "foreground": "#A3A094b0"
                    }
                },
                {
                    "name": "CSS Properties",
                    "scope": [
                        "support.type.property-name"
                    ],
                    "settings": {
                        "fontStyle": "normal",
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "name": "Search Results Nums",
                    "scope": [
                        "constant.numeric.line-number.find-in-files - match"
                    ],
                    "settings": {
                        "foreground": "#A3A094b0"
                    }
                },
                {
                    "name": "Search Results Match Nums",
                    "scope": [
                        "constant.numeric.line-number.match"
                    ],
                    "settings": {
                        "foreground": "#E6DCB9"
                    }
                },
                {
                    "name": "Search Results Lines",
                    "scope": [
                        "entity.name.filename.find-in-files"
                    ],
                    "settings": {
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "scope": [
                        "message.error"
                    ],
                    "settings": {
                        "foreground": "#A4C6A9"
                    }
                },
                {
                    "name": "Markup heading",
                    "scope": [
                        "markup.heading",
                        "markup.heading entity.name"
                    ],
                    "settings": {
                        "fontStyle": "bold",
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "name": "Markup links",
                    "scope": [
                        "markup.underline.link",
                        "string.other.link"
                    ],
                    "settings": {
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "name": "Markup Italic",
                    "scope": [
                        "markup.italic"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#66AFA0"
                    }
                },
                {
                    "name": "Markup Bold",
                    "scope": [
                        "markup.bold"
                    ],
                    "settings": {
                        "fontStyle": "bold",
                        "foreground": "#66AFA0"
                    }
                },
                {
                    "name": "Markup Bold/italic",
                    "scope": [
                        "markup.italic markup.bold",
                        "markup.bold markup.italic"
                    ],
                    "settings": {
                        "fontStyle": "bold italic"
                    }
                },
                {
                    "name": "Markup Code",
                    "scope": [
                        "markup.raw"
                    ],
                    "settings": {
                        "background": "#EAE5D406"
                    }
                },
                {
                    "name": "Markup Code Inline",
                    "scope": [
                        "markup.raw.inline"
                    ],
                    "settings": {
                        "background": "#EAE5D410"
                    }
                },
                {
                    "name": "Markdown Separator",
                    "scope": [
                        "meta.separator"
                    ],
                    "settings": {
                        "fontStyle": "bold",
                        "background": "#EAE5D410",
                        "foreground": "#A3A094b0"
                    }
                },
                {
                    "name": "Markup Blockquote",
                    "scope": [
                        "markup.quote"
                    ],
                    "settings": {
                        "foreground": "#E6DCB9",
                        "fontStyle": "italic"
                    }
                },
                {
                    "name": "Markup List Bullet",
                    "scope": [
                        "markup.list punctuation.definition.list.begin"
                    ],
                    "settings": {
                        "foreground": "#CAB2A1"
                    }
                },
                {
                    "name": "Markup added",
                    "scope": [
                        "markup.inserted"
                    ],
                    "settings": {
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "name": "Markup modified",
                    "scope": [
                        "markup.changed"
                    ],
                    "settings": {
                        "foreground": "#CAB2A1"
                    }
                },
                {
                    "name": "Markup removed",
                    "scope": [
                        "markup.deleted"
                    ],
                    "settings": {
                        "foreground": "#A4C6A9"
                    }
                },
                {
                    "name": "Markup Strike",
                    "scope": [
                        "markup.strike"
                    ],
                    "settings": {
                        "foreground": "#D5D2B1"
                    }
                },
                {
                    "name": "Markup Table",
                    "scope": [
                        "markup.table"
                    ],
                    "settings": {
                        "background": "#EAE5D410",
                        "foreground": "#9FAB99"
                    }
                },
                {
                    "name": "Markup Raw Inline",
                    "scope": [
                        "text.html.markdown markup.inline.raw"
                    ],
                    "settings": {
                        "foreground": "#CAB2A1"
                    }
                },
                {
                    "name": "Markdown - Line Break",
                    "scope": [
                        "text.html.markdown meta.dummy.line-break"
                    ],
                    "settings": {
                        "background": "#A3A094b0",
                        "foreground": "#A3A094b0"
                    }
                },
                {
                    "name": "Markdown - Raw Block Fenced",
                    "scope": [
                        "punctuation.definition.markdown"
                    ],
                    "settings": {
                        "background": "#EAE5D4",
                        "foreground": "#A3A094b0"
                    }
                }
            ]
        }
    },
